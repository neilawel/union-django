{
    "version": 2,
    "builds": [
      {
        "src": "myproject/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "build": {
      "env": {
        "DJANGO_SETTINGS_MODULE": "myproject.settings"
      },
      "commands": [
        "python3 manage.py collectstatic --noinput"
      ]
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ]
  }
  