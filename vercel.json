{
    "version": 2,
    "builds": [
      {
        "src": "myproject/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb", 
          "runtime": "python" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ]
  }
  